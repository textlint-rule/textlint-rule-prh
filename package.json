{
    "name": "textlint-rule-prh",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/textlint-rule/textlint-rule-prh.git"
    },
    "author": "azu",
    "email": "azuciao@gmail.com",
    "homepage": "https://github.com/textlint-rule/textlint-rule-prh",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/textlint-rule/textlint-rule-prh/issues"
    },
    "type": "module",
    "version": "6.1.0",
    "description": "textlint rule for prh.",
    "main": "lib/textlint-rule-prh.js",
    "exports": {
        ".": {
            "import": "./dist/node.js",
            "require": "./dist/node.cjs",
            "browser": "./dist/browser.js",
            "types": {
                "import": "./dist/node.d.ts",
                "require": "./dist/node.d.cts",
                "browser": "./dist/browser.d.ts"
            }
        }
    },
    "files": [
        "lib",
        "src"
    ],
    "directories": {
        "test": "test"
    },
    "scripts": {
        "build": "tsdown",
        "watch": "tsdown --watch",
        "prepublish": "npm run --if-present build",
        "test": "mocha --require textlint-scripts/register \"test/**/*.js\"",
        "format": "prettier --write \"**/*.{js,jsx,ts,tsx,css}\"",
        "prepare": "git config --local core.hooksPath .githooks"
    },
    "keywords": [
        "textlint",
        "prh"
    ],
    "dependencies": {
        "@babel/parser": "^7.28.0",
        "prh": "^5.4.4",
        "textlint-rule-helper": "^2.3.1",
        "textlint-tester": "^15.2.1"
    },
    "devDependencies": {
        "@textlint/legacy-textlint-core": "^15.2.1",
        "@textlint/types": "^15.2.1",
        "@types/mocha": "^10.0.10",
        "@types/node": "^24.3.0",
        "lint-staged": "^15.5.2",
        "mocha": "^11.7.1",
        "prettier": "^3.5.3",
        "textlint": "15.2.1",
        "textlint-scripts": "^15.2.1",
        "tsdown": "^0.14.1",
        "typescript": "^5.9.2"
    },
    "prettier": {
        "singleQuote": false,
        "printWidth": 120,
        "tabWidth": 4,
        "trailingComma": "none"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx,css}": [
            "prettier --write"
        ]
    },
    "husky": {
        "hooks": {
            "post-commit": "git reset",
            "pre-commit": "lint-staged"
        }
    },
    "packageManager": "pnpm@10.14.0+sha512.ad27a79641b49c3e481a16a805baa71817a04bbe06a38d17e60e2eaee83f6a146c6a688125f5792e48dd5ba30e7da52a5cda4c3992b9ccf333f9ce223af84748"
}
