{
  "name": "textlint-rule-prh",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/textlint-rule/textlint-rule-prh.git"
  },
  "author": "azu",
  "email": "azuciao@gmail.com",
  "homepage": "https://github.com/textlint-rule/textlint-rule-prh",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/textlint-rule/textlint-rule-prh/issues"
  },
  "version": "5.0.1",
  "description": "textlint rule for prh.",
  "main": "lib/textlint-rule-prh.js",
  "files": [
    "lib",
    "src"
  ],
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "NODE_ENV=production babel src --out-dir lib --source-maps",
    "watch": "babel src --out-dir lib --watch --source-maps",
    "prepublish": "npm run --if-present build",
    "test": "mocha",
    "precommit": "lint-staged",
    "postcommit": "git reset",
    "prettier": "prettier --write '**/*.{js,jsx,ts,tsx,css}'"
  },
  "keywords": [
    "textlint",
    "prh"
  ],
  "dependencies": {
    "prh": "^5.4.3",
    "textlint-rule-helper": "^2.0.0",
    "untildify": "^3.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-jsdoc-to-assert": "^4.0.0",
    "babel-preset-power-assert": "^1.0.0",
    "babel-register": "^6.26.0",
    "husky": "^0.14.3",
    "lint-staged": "^6.0.0",
    "mocha": "^4.0.0",
    "power-assert": "^1.4.4",
    "prettier": "^1.7.0",
    "textlint": "^9.1.1",
    "textlint-tester": "^3.0.0"
  },
  "peerDependencies": {
    "textlint": ">= 5.5.0"
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 4
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css}": [
      "prettier --write",
      "git add"
    ]
  }
}